<div class="container">
  <p id="notice"><%= notice %></p>

  <div class="title">
    <h1>Lists</h1>
  </div>
  <div class="cards">
    <% @lists.each do |list| %>
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://source.unsplash.com/1600x900/?<%= list.name %>)">
        <%= link_to list.name, list_path(list) %>
      </div>  
    <% end %> 
  </div>
  <%= link_to 'New List', new_list_path %>
</div>